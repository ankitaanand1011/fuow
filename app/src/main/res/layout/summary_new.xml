                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:id="@+id/tv_homepage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Home Page "
                        android:textColor="@color/dark_grey"
                        android:textSize="12sp"
                        />
                    <TextView
                        android:id="@+id/tv_homepage_val"
                        android:layout_toEndOf="@+id/tv_homepage"
                        android:layout_width="match_parent"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/dark_grey"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:gravity="right"
                        />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/rl_exibition"
                    android:layout_below="@+id/rl_product"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:id="@+id/tv_exibition"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Homepage exhibition and conferences"
                        android:textColor="@color/dark_grey"
                        android:textSize="12sp"
                        />
                    <TextView
                        android:id="@+id/tv_exibition_val"
                        android:layout_toEndOf="@+id/tv_exibition"
                        android:layout_width="match_parent"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/dark_grey"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:gravity="right"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_header"
                    android:layout_below="@+id/rl_product"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:id="@+id/tv_header_banner"
                        android:layout_width="wrap_content"
