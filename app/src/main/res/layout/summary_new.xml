            <LinearLayout
                android:id="@+id/ll_quick_statics"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"

                android:orientation="vertical"
                android:background="@color/white"
                >
                <RelativeLayout
                    android:id="@+id/rl_quick_statistics_new"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/tv_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_quick_statistics"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Total Impression "
                        android:textColor="@color/dark_grey"
                        android:textSize="12sp"
                        />
                    <LinearLayout
                        android:layout_toRightOf="@+id/tv_quick_statistics"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="right"
                         >

                        <TextView
                            android:id="@+id/tv_quick_statistics_edit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"

                            android:textColor="#323232"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:background="@drawable/circle_grey"/>
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_total_impression"
                    android:layout_below="@+id/tv_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:id="@+id/tv_impression"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Total Search Impression"
                        android:textColor="@color/dark_grey"
                        android:textSize="12sp"
                        />
                    <LinearLayout
                        android:layout_toRightOf="@+id/tv_impression"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="right"
                        >

                        <TextView
                            android:id="@+id/tv_impression_val"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"

                            android:textColor="#323232"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:background="@drawable/circle_grey"/>
                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_total_search_impresson"
                    android:layout_below="@+id/tv_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:id="@+id/tv_total_search_impression"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Impression in the last week"
                        android:textColor="@color/dark_grey"
                        android:textSize="12sp"
                        />
                    <LinearLayout
                        android:layout_toRightOf="@+id/tv_total_search_impression"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="right"
                        >

                        <TextView
                            android:id="@+id/tv_total_search_impression_val"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"

                            android:textColor="#323232"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:background="@drawable/circle_grey"/>
                    </LinearLayout>

                    <!--
                                        <TextView
                                            android:id="@+id/tv_total_search_impression_val"
                                            android:layout_toEndOf="@+id/tv_total_search_impression"
                                            android:layout_width="match_parent"
                                            android:layout_marginStart="10dp"
                                            android:textColor="@color/dark_grey"
                                            android:layout_height="wrap_content"
                                            android:textSize="12sp"
                                            android:gravity="right"
                                            />
                    -->
                </RelativeLayout>


            </LinearLayout>


        </LinearLayout>
    </ScrollView>


</LinearLayout>
